{
    "name":"Crypto Wallet",
    "description":"A secure wallet for managing your cryptocurrencies.",
    "version":"1.0",
    "manifest_version": 3,
    "action":{
        "default_popup": "popup.html",
        "default_icon": "icons/icon.png"
    },
    "icons":{
        "16": "icons/icon.png",
        "32": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    },
    "permissions": [
        "tabs",
        "bookmarks",
        "unlimitedStorage",
        "nativeMessaging"
    ]
}